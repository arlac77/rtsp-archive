{
  "name": "rtsp-archive",
  "version": "0.0.0-semantic-release",
  "main": "dist/rtsp-archive.js",
  "module": "src/rtsp-archive.js",
  "description": "archive rtsp stream with openRTSP",
  "keywords": [
    "rtsp"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "rtsp-archive": "bin/rtsp-archive"
  },
  "scripts": {
    "precover": "rollup -c tests/rollup.config.js",
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "docs": "documentation readme src/rtsp-archive.js --section=API",
    "semantic-release": "semantic-release",
    "prepare": "rollup -c && chmod +x bin/rtsp-archive",
    "lint": "documentation lint src/rtsp-archive.js"
  },
  "dependencies": {
    "async": "2.6.0",
    "caporal": "^0.9.0",
    "config-expander": "9.1.0",
    "del": "^3.0.0",
    "make-dir": "^1.1.0",
    "mdns": "^2.3.4"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "execa": "^0.8.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.4.1",
    "rollup": "^0.53.4",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "semantic-release": "^11.1.0",
    "documentation": "^5.3.5"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/rtsp-archive.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/rtsp-archive/issues"
  },
  "homepage": "https://github.com/arlac77/rtsp-archive#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
