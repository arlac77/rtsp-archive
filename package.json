{
  "name": "rtsp-archive",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "module": "src/rtsp-archive.mjs",
  "description": "archive RTSP stream(s) with openRTSP or ffmpeg",
  "keywords": [
    "rtsp"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "rtsp-archive": "bin/rtsp-archive"
  },
  "scripts": {
    "precover": "rollup -c tests/rollup.config.js",
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "docs": "documentation readme src/rtsp-archive.mjs --section=API",
    "semantic-release": "semantic-release",
    "prepare": "rollup -c",
    "lint": "documentation lint src/rtsp-archive.mjs",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "caporal": "^0.10.0",
    "config-expander": "^9.1.8",
    "make-dir": "^1.3.0",
    "mdns": "^2.3.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.1.0",
    "ava": "^1.0.0-beta.8",
    "documentation": "^8.1.2",
    "execa": "^1.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^13.1.0",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-cleanup": "^3.0.0",
    "semantic-release": "^15.9.16",
    "travis-deploy-once": "^5.0.9"
  },
  "engines": {
    "node": ">=10.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/rtsp-archive.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/rtsp-archive/issues"
  },
  "homepage": "https://github.com/arlac77/rtsp-archive#readme",
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "build/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.mjs"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
