{
  "name": "rtsp-archive",
  "version": "0.0.0-semantic-release",
  "description": "archive rtsp stream with openRTSP",
  "module": "src/rtsp-archive.js",
  "main": "dist/rtsp-archive.js",
  "keywords": [
    "rtsp"
  ],
  "bin": {
    "rtsp-archive": "bin/rtsp-archive"
  },
  "scripts": {
    "precover": "npm run prepublish",
    "cover": "nyc ava",
    "test": "ava",
    "pretest": "rollup -c rollup.config.test.js",
    "posttest": "markdown-doctest",
    "prepublish": "rollup --output=bin/rtsp-archive -c -- src/rtsp-archive.js && chmod +x bin/rtsp-archive",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/rtsp-archive.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/rtsp-archive/issues"
  },
  "homepage": "https://github.com/arlac77/rtsp-archive#readme",
  "dependencies": {
    "async": "^2.2.0",
    "caporal": "^0.4.0",
    "config-expander": "^7.0.1",
    "mdns": "^2.3.3",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-preset-es2015-rollup": "^3.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "execa": "^0.6.3",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^10.1.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "semantic-release": "^6.3.6"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=7.7.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "files": [
      "build/test-bundle.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/test-bundle.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  }
}
