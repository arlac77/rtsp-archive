{
  "name": "rtsp-archive",
  "version": "0.0.0-semantic-release",
  "main": "dist/rtsp-archive.js",
  "module": "src/rtsp-archive.js",
  "description": "archive rtsp stream with openRTSP",
  "keywords": ["rtsp"],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "rtsp-archive": "bin/rtsp-archive"
  },
  "scripts": {
    "precover": "rollup -c tests/rollup.config.js",
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "pretest":
      "rollup -c tests/rollup.config.js && rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "docs": "documentation readme src/rtsp-archive.js --section=API",
    "semantic-release": "semantic-release",
    "prepare": "rollup -c",
    "lint": "documentation lint src/rtsp-archive.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "caporal": "^0.10.0",
    "config-expander": "9.1.2",
    "make-dir": "^1.2.0",
    "mdns": "^2.3.4"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "documentation": "^6.3.1",
    "execa": "^0.10.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.7.1",
    "rollup": "^0.58.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-executable": "^1.2.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "semantic-release": "^15.1.7",
    "travis-deploy-once": "^4.4.1"
  },
  "engines": {
    "node": ">=8.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/rtsp-archive.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/rtsp-archive/issues"
  },
  "homepage": "https://github.com/arlac77/rtsp-archive#readme",
  "ava": {
    "files": ["build/*-test.js"],
    "require": ["babel-register"]
  },
  "nyc": {
    "include": ["build/*-test.js", "src/**/*.js"],
    "reporter": ["lcov"],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
