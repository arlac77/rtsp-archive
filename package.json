{
  "name": "rtsp-archive",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "src/rtsp-archive.mjs",
  "description": "archive RTSP stream(s) with openRTSP or ffmpeg",
  "keywords": [
    "rtsp"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "rtsp-archive": "bin/rtsp-archive"
  },
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "test": "ava",
    "posttest": "npm run prepare && markdown-doctest",
    "docs": "documentation readme src/rtsp-archive.mjs --section=API",
    "semantic-release": "semantic-release",
    "prepare": "rollup -c",
    "package": "npm-pkgbuild -w build",
    "lint": "documentation lint src/rtsp-archive.mjs",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "caporal": "^1.1.0",
    "config-expander": "^9.1.9",
    "mdns": "^2.3.4"
  },
  "devDependencies": {
    "ava": "^1.0.0-rc.2",
    "c8": "^3.2.1",
    "documentation": "^9.0.0",
    "esm": "^3.0.84",
    "execa": "^1.0.0",
    "markdown-doctest": "^0.9.1",
    "npm-pkgbuild": "^1.1.1",
    "rollup": "^0.67.4",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "semantic-release": "^15.12.5",
    "travis-deploy-once": "^5.0.10"
  },
  "engines": {
    "node": ">=10.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/rtsp-archive.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/rtsp-archive/issues"
  },
  "homepage": "https://github.com/arlac77/rtsp-archive#readme",
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
